package main 

@(extern)
printf :: func(msg: string, a: int, b: int)

fib :: func(n: int) -> int {
    if n <= 1 {
        return n 
    }

    return fib(n - 1) + fib(n - 2)
}

@(export)
main :: func() -> int {
    n := 1

    for n <= 10 {
        printf("Fibonacci of %2d is: %2d\n", n, fib(n))

        n = n + 1
    }

    return 0
}