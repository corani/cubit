package main

@(extern)
printf :: func(msg: string)

@(export)
main :: func() -> int {
    for i := 0; i <= 1; i = i + 1 {
        for j := 0; j <= 1; j = j + 1 {
            if i == 1 && j == 1 {
                printf("Both i and j are true\n")
            } else if i == 1 || j == 1 {
                printf("Either i or j is true\n")
            } else {
                printf("Neither i nor j is true\n")
            }
        }
    }

    return 0
}